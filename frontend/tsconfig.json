    "outDir": "dist",
    "allowJs": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "types": ["vitest/globals", "@testing-library/jest-dom"],
    "erasableSyntaxOnly": false,
    "exactOptionalPropertyTypes": false
  },
  "include": ["src", "vite-env.d.ts"],
  "exclude": ["src/oapi"],
  "extends": [
    "@tsconfig/vite-react/tsconfig.json",
    "@tsconfig/strictest/tsconfig.json"
  ]
}
